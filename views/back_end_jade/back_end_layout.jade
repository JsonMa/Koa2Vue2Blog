doctype html
html
    head(lang='zh-cmn-Hans')
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        |     {#Tell the browser to be responsive to screen width#}
        meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
        |     {# Bootstrap 3.3.6 #}
        link(rel='stylesheet', href='css/bootstrap.min.css')
        |     {# Font Awesome #}
        link(rel='stylesheet', href='css/font-awesome.css')
        |     {# Theme style #}
        link(rel='stylesheet', href='css/AdminLTE.css')
        |     {# AdminLTE Skins. Choose a skin from the css/skins folder instead of downloading all of them to reduce the load. #}
        link(rel='stylesheet', href='css/skins/_all-skins.min.css')
        //if lt IE 9
          script(src='https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js')
          script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
        |     {# 这是放置title和描述、关键字的区域 #}
        |     {% block title %}
        title {{ title }}
        |     {% endblock %}
        |     {# 这是放置私有的common less文件的引用 #}
        link(href='./css/common.less', rel='stylesheet')
        // STYLE_PLACEHOLDER
        | {% block css %}
        | {% endblock %}
    body.hold-transition.skin-blue.sidebar-mini
        .wrapper
            | {# 这是公用头部 #}
            | {% block header %}
            | {% include "./header.html" %}
            | {% endblock %}
            | {# 这是主体内容 #}
            | {% block wrapper %}
            | {% endblock %}
            | {# 这是公用尾部 #}
            | {% block footer %}
            | {% include "./footer.html" %}
            | {% endblock %}
        | {# jQuery 2.2.3 #}
        script(src='../plugins/jQuery/jquery-2.2.3.min.js')
        | {# jQuery UI 1.11.4 #}
        script(src='../plugins/jQueryUI/jquery-ui.min.js')
        | {# Bootstrap 3.3.6 #}
        script(src='./js/bootstrap.min.js')
        script(src='./js/app.js')
        script(src='./js/common.js')
        script.
            $(function () {
                $('.treeview-menu').find('.active').parent('.treeview-menu').addClass('menu-open').css('display', 'block');
            })
        // SCRIPT_PLACEHOLDER
        | {# 这是私有js文件 #}
        | {% block js %}
        | {% endblock %}
